@{
    Layout = "~/Views/Shared/Student.cshtml";
}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Classroom</title>
    <!-- Bootstrap CSS -->
    <link href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css" rel="stylesheet">
    <script src="https://kit.fontawesome.com/8ee0e83968.js" crossorigin="anonymous"></script>
    <style>
        body {
            background-color: darkblue;
            font-family: 'Arial', sans-serif;
        }

        .header {
            background-color: #0D6EFD;
            color: white;
            padding: 20px;
            text-align: center;
            transition: background-color 0.3s ease;
        }

            .header:hover {
                background-color: #0948ba;
            }

        .container {
            margin-top: 30px;
        }

        .card {
            margin-bottom: 20px;
            transition: transform 0.3s ease, box-shadow 0.3s ease;
        }

            .card:hover {
                transform: translateY(-10px);
                box-shadow: 0 8px 16px rgba(0, 0, 0, 0.2);
            }

        .form-control {
            margin-bottom: 10px;
            transition: border-color 0.3s ease;
        }

            .form-control:focus {
                border-color: #0D6EFD;
                box-shadow: 0 0 5px rgba(13, 110, 253, 0.5);
            }

        .section-title {
            margin-top: 30px;
            margin-bottom: 20px;
            text-align: center;
            color: black;
            font-weight: bold;
        }

        .btn-primary {
            background-color: #0D6EFD;
            border-color: #0D6EFD;
            transition: background-color 0.3s ease, box-shadow 0.3s ease;
        }

            .btn-primary:hover {
                background-color: #0948ba;
                box-shadow: 0 8px 16px rgba(0, 0, 0, 0.2);
            }

        .btn-center {
            display: flex;
            justify-content: center;
        }

        .announcement-section {
            padding: 20px;
            background-color: #f9f9f9;
            border-radius: 5px;
        }

        .Background-clip-container {
            position: fixed;
            width: 100%;
            height: 100%;
            overflow: hidden;
            z-index: -1;
            display: grid;
            grid-template-columns: repeat(auto-fill, minmax(100%, 1fr));
            grid-template-rows: repeat(auto-fill, minmax(100%, 1fr));
        }

        .Background-clip {
            width: 100%;
            height: fit-content;
            object-fit: cover;
        }

        .sidebar {
            position: fixed;
            top: 0;
            left: 0;
            height: 100%;
            width: 250px;
            background-color: #333;
            color: white;
            padding-top: 20px;
        }

            .sidebar a {
                padding: 10px 15px;
                text-decoration: none;
                font-size: 18px;
                color: white;
                display: block;
                transition: 0.3s;
            }

                .sidebar a:hover {
                    background-color: blueviolet;
                }


        .main-content {
            margin-left: 260px;
            padding: 20px;
        }
    </style>
</head>
